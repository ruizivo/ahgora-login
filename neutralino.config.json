{
  "applicationId": "ahgora.login",
  "version": "0.0.0",
  "defaultMode": "window",
  "port": 3000,
  "documentRoot": "/app/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "storage.*"
  ],
  "modes": {
    "window": {
      "title": "Ahgora login ",
      "width": 800,
      "height": 500,
      "minWidth": 800,
      "minHeight": 500,
      "maxWidth": 800,
      "maxHeight": 500,
      "resizable":false,
      "icon": "/app/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "ahgora-login",
    "resourcesPath": "/app/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/app/public/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0",
    "frontendLibrary": {
      "patchFile": "/app/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}